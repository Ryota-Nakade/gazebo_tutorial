<launch>
    <arg name="model" default="$(find gazebo_tutorial)/urdf/simple_body1_gazebo.urdf"/>
    <param name="robot_description" command="$(find xacro)/xacro $(arg model)"/>

    <include file="$(find gazebo_ros)/launch/empty_world.launch"/>

    <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model test_robot -param robot_description"/>
</launch>
